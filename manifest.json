{
  "name": "Old fonts in F1",
  "version": "1.0.0",
  "description": "Old fonts in F1",
  "homepage_url": "https://github.com/ktomov/old-fonts-in-f1",
  "manifest_version": 2,
  "permissions": [
    "*://*.formula1.com/*"
  ],
  "icons": {
    "128": "icon.png"
  },
  "content_scripts": [
    {
      "run_at": "document_start",
      "matches": [
        "*://*.formula1.com/*"
      ],
      "css": [
        "old-font.css"
      ]
    }
  ]
}
